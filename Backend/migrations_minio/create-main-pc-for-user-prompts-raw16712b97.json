  "id": "create-main-pc-for-user-prompts-raw-16712b97",
  "created": "2025-04-02T15:57:29.869515",
  "order": 1,
  "actions": [
    {
      "type": "generate_policy_config",
      "config_name": "main_for_user_prompts_raw",
      "policy": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": [
              "s3:PutObject",
              "s3:GetObject",
              "s3:DeleteObject",
              "s3:ListMultipartUploadParts",
              "s3:AbortMultipartUpload",
              "s3:RestoreObject"
            ],
            "Resource": [
              "arn:aws:s3:::user-prompts/raw/*"
            ]
          },
          {
            "Effect": "Allow",
            "Action": [
              "s3:GetObject"
            ],
            "Resource": [
              "arn:aws:s3:::user-prompts/result/*"
            ]
          },
          {
            "Effect": "Allow",
            "Action": [
              "s3:ListBucket"
            ],
            "Resource": [
              "arn:aws:s3:::user-prompts"
            ]
          }
        ]
      }
    }
  ]
}